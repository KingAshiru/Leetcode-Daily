class Solution:
    def squareIsWhite(self, coordinates: str) -> bool:
        check = {'a','c','e','g'}
        if coordinates[0] in check:
            if int(coordinates[1]) % 2 == 1:
                return False
            else:
                return True
        else:
            if int(coordinates[1]) % 2 == 1:
                return True
            else:
                return False
